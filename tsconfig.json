{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "es6",
        "target": "es5",
        "moduleResolution": "node",
        "lib": [
            "webworker",
            "es6",
            "scripthost"
        ],
        "allowJs": true,
        "noImplicitAny": true,
        "sourceMap": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
